{
  "private": true,
  "name": "derrxb",
  "description": "",
  "license": "",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run build:css && cross-env NODE_ENV=production remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "cross-env NODE_ENV=development remix build && run-p dev:*",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:node": "cross-env NODE_ENV=development PORT=3000 nodemon ./build/index.js",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production PORT=8080 node ./build/index.js"
  },
  "dependencies": {
    "@remix-run/express": "^1.2.3",
    "@remix-run/react": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@types/js-cookie": "^3.0.1",
    "apicache": "^1.6.3",
    "cloudinary": "^1.27.1",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "framer-motion": "^5.3.3",
    "js-cookie": "^3.0.1",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.32.5",
    "remix": "^1.2.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.9.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false
}
